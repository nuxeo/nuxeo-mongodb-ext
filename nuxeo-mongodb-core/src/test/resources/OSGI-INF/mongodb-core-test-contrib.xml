<?xml version="1.0"?>
<component name="org.nuxeo.mongodb.core.test.contrib">

  <require>org.nuxeo.mongodb.core.MongoDBComponent</require>
  <extension target="org.nuxeo.mongodb.core.MongoDBComponent" point="connection">
    <connection id="default">
      <server>${nuxeo.test.mongodb.server}</server>
      <dbname>${nuxeo.test.mongodb.dbname}</dbname>
    </connection>
  </extension>

</component>
